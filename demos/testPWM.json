{
  "states": {
    "n0": {
      "name": "add",
      "events": [
        {
          "trigger": {
            "component": "System",
            "method": "onEnter"
          },
          "do": [
            {
              "component": "timer",
              "method": "start",
              "args": {
                "interval": "500"
              }
            }
          ]
        },
        {
          "trigger": {
            "component": "timer",
            "method": "timeout"
          },
          "do": [
            {
              "component": "counter",
              "method": "add",
              "args": {
                "value": "5"
              }
            },
            {
              "component": "pwm",
              "method": "write",
              "args": {
                "value": {
                  "component": "counter",
                  "method": "value"
                }
              }
            },
            {
              "component": "serial",
              "method": "println",
              "args": {
                "value": {
                  "component": "counter",
                  "method": "value"
                }
              }
            }
          ]
        }
      ],
      "bounds": {
        "x": 196,
        "y": 364,
        "height": 95,
        "width": 450
      }
    },
    "n1": {
      "name": "sub",
      "events": [
        {
          "trigger": {
            "component": "timer",
            "method": "timeout"
          },
          "do": [
            {
              "component": "counter",
              "method": "sub",
              "args": {
                "value": "5"
              }
            },
            {
              "component": "pwm",
              "method": "write",
              "args": {
                "value": {
                  "component": "counter",
                  "method": "value"
                }
              }
            },
            {
              "component": "serial",
              "method": "println",
              "args": {
                "value": {
                  "component": "counter",
                  "method": "value"
                }
              }
            }
          ]
        }
      ],
      "bounds": {
        "x": 196,
        "y": 364,
        "height": 95,
        "width": 450
      }
    }
  },

  "initialState": {
    "target": "n0",
    "position": {
      "x": 96,
      "y": 264
    }
  },

  "transitions": [
    {
      "source": "n0",
      "target": "n1",
      "color": "#F29727",
      "position": {
        "x": 361,
        "y": 127
      },
      "trigger": {
        "component": "counter",
        "method": "isEqual",
        "args": {
          "value": "255"
        }
      },
      "condition": null,
      "do": []
    },
    {
      "source": "n1",
      "target": "n0",
      "color": "#F29727",
      "position": {
        "x": 361,
        "y": 127
      },
      "trigger": {
        "component": "counter",
        "method": "isEqual",
        "args": {
          "value": "0"
        }
      },
      "condition": null,
      "do": []
    }
  ],

  "components": {
    "timer": {
      "type": "Timer",
      "parameters": {}
    },
    "counter": {
      "type": "Counter",
      "parameters": {}
    },

    "serial": {
      "type": "QHsmSerial",
      "parameters": {
        "baud": "9600"
      }
    },
    "pwm": {
      "type": "PWM",
      "parameters": {
        "pin": "6"
      }
    }
  },
  "compilerSettings": {
    "filename": "ExampleSketch",
    "compiler": "arduino-cli",
    "flags": ["-b", "arduino:avr:uno"]
  },

  "platform": "ArduinoUno",
  "parameters": {}
}
